{
  "name": "com-pog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vitepress dev src --port=3182",
    "build": "yarn gen && vitepress build src && touch dist/.nojekyll",
    "lint": "yarn _format --check && eslint src/vite.config.ts \"src/.vitepress/**/*.{ts,tsx,vue}\"",
    "format": "yarn _format --write",
    "_format": "prettier \"scripts/**/*.js\" \"src/**/*.{ts,md}\" \"src/**/*.{ts,tsx,vue,css,md}\"",
    "gen": "node scripts"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.2.0",
    "markdown-it-katex": "^2.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vitepress": "^1.0.0-alpha.4",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.0"
  },
  "packageManager": "yarn@3.2.1"
}
