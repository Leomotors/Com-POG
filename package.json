{
  "name": "com-pog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vitepress dev src --port=3182",
    "build": "pnpm gen && vitepress build src && touch dist/.nojekyll && cp robots.txt dist",
    "lint": "pnpm _format --check && eslint src/vite.config.ts \"src/.vitepress/**/*.{ts,tsx,vue}\"",
    "format": "pnpm _format --write",
    "_format": "prettier \"scripts/**/*.js\" \"src/**/*.{ts,md}\" \"src/**/*.{ts,tsx,vue,css,md}\"",
    "gen": "node scripts",
    "deps": "ncu -u !katex"
  },
  "engines": {
    "pnpm": "^7"
  },
  "devDependencies": {
    "@leomotors/cocoa-vitepress-theme": "^1.0.2",
    "@types/node": "^18.6.5",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "katex": "^0.6.0",
    "markdown-it-katex": "^2.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "vitepress": "1.0.0-alpha.4",
    "vue": "^3.2.37"
  }
}