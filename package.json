{
  "name": "com-pog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vitepress dev src --port=3182",
    "build": "pnpm gen && vitepress build src",
    "lint": "pnpm _format --check && eslint src/vite.config.ts \"src/.vitepress/**/*.{ts,tsx,vue}\"",
    "format": "pnpm _format --write",
    "_format": "prettier \"scripts/**/*.ts\" \"src/**/*.{ts,md}\" \"src/**/*.{ts,tsx,vue,css,md}\"",
    "gen": "tsx scripts/index.ts"
  },
  "engines": {
    "pnpm": "^7"
  },
  "devDependencies": {
    "@leomotors/cocoa-vitepress-theme": "^1.0.2",
    "@octokit/types": "^7.1.0",
    "@types/node": "^18.7.6",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.8",
    "clsx": "^1.2.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "katex": "^0.6.0",
    "markdown-it-katex": "^2.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "undici": "^5.9.1",
    "vite": "^3.0.8",
    "vitepress": "1.0.0-alpha.8",
    "vue": "^3.2.37"
  }
}